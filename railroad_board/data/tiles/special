<spc>

TYPES {
    T
}

TILES {
    <a> E: E, (E, E, E, E), _ _     # Empty tile

    <b> S: E, (C, C, C, C), _ _     # Special tiles
    <c> S: E, (C, C, C, R), S _
    <d> S: E, (C, C, R, R), S _
    <e> S: E, (C, R, C, R), S _
    <f> S: E, (C, R, R, R), S _
    <g> S: E, (R, R, R, R), _ _

    <h> i: E, (C, E, E, E), _ _     # Goal tiles
    <i> i: E, (R, E, E, E), _ _
}

DICE {
    <special_dice> (b, c, d, e, f, g)
}

