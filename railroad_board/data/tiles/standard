EXTRA: # Extra types
E;     # Empty (name *cannot* be changed)

# id: type (north_connection, east_connection, south_connection, west_connection)
# an S is added to the last part of the tile if it contains a station
C: E (C C E E)
C: E (R R E E)
C: E (C R E E) S

i: E (C E E E) S
i: E (R E E E) S

I: E (C E C E)
I: E (R E R E)
I: E (C E R E) S

T: E (C C C E)
T: E (R R R E)
T: E (C R C E) S
T: E (R C R E) S
T: E (C C R E) S
T: E (C R R E) S

CC: E (C C C C)
CC: E (R R R R)

O: E (C R C R)

